<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.controllers.RankingController">
    <children>
        <Label style="-fx-font-size: 20; -fx-font-weight: bold;" text="Ranking" />
        <ListView fx:id="rankingList" prefHeight="500" prefWidth="300" />
        <Button onAction="#goToHome" text="Back to Home" />
    </children>
</VBox>
